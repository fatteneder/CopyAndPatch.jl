#!/usr/bin/env bash

llvm-readobj --elf-output-style=JSON \
             --expand-relocs \
             --pretty-print \
             --section-data \
             --section-relocations \
             --section-symbols \
             --sections \
             $1
             # --symbols \
             # --demangle \
             # --section-details \
             # --section-headers \
             # --section-mapping \
